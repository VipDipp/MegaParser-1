    margin-top: -1px;
    background: white;
    height: 1px;
}
.close:before {
    transform: rotate(45deg);
}
.close:after {
    transform: rotate(-45deg);
}

/* Credits */
.credits {
    text-align: right;
    padding: 0.5em;
    line-height: 1em;
    opacity: 0.75;
    transition: opacity 0.2s linear;
}
.credits:hover {
    opacity: 1;
}
@media only screen and (max-width: 767px) {
    .credits {
        display: none;
    }
}

/* Hide header, reveal footer */
header {
    position: fixed;
    z-index: 1;
    background: white;
    top: 0;
    left: 0;
    height: 2em;
    width: 100%;
}
header + main {
    margin-top: 2em;
}
main {
    overflow: auto;
    z-index: 2;
    background: transparent;
    position: relative;
}
main:nth-last-child(3) {
    margin-bottom: 50vh;
}
footer {
    height: 50vh;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    background: whitesmoke;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 1;
}
@media only screen and (max-width: 767px) {
    header {
        height: auto;
        position: relative;
    }
    header + main {
        margin-top: 0;
    }
    main:nth-last-child(3) {
        margin-bottom: 0;
    }
    footer {
        height: auto;
        position: relative;
        padding: 1em;
    }
}
@media print {
    header,
    footer {
        display: none;
    }
    header + main {
        margin-top: 0;
    }
    main:nth-last-child(3) {
        margin-bottom: 0;
    }
}
</style>
<style>
    body {
    background: rgb(10,10,17);
    background: linear-gradient(180deg, rgba(10,10,17,1) 0%, rgba(65,67,68,1) 100%);
}
.page {
    background-color: rgba(65,67,68, 1);
    color: white !important;
}

.skill {
    display: flex;
    align-items: center;
}


.skill img {
    margin-right: 1em;
}
.skill span {
    max-width: 300px;
}

.talents #talentImg {
    max-height: 60%;
    max-width: 60%;
}
.rowSide {
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.row {
    display: flex;
}

.inline-img {
    display: inline-flex;
    align-items: center;
    font-weight: 500;
    margin-right: 2em;
}
.inline-img img {
    margin-right: .5em;
}

.startStats ul li {
    list-style-type: "" !important;
}

.pl-1{
    margin-left: 1em;
}
.pl-2{
    margin-left: 2em !important;
}
.pt-1{
    margin-top: 1em;
}

</style>
<main>
    <%if (hero) {%>
  <article>
    <div class="page" style="border-color: #a8a875; border-radius: 1em;">
      <div class="stackable grid">
        <div class="row">
          <div class="four wide center aligned column">
            <div class="avatar-wrapper">
              <img class="avatar" src="<%= hero.img %>" role="img">
            </div>
          </div>
          <div class="twelve wide column">
            <div class="stackable grid">
              <div class="sixteen wide column">
                <h1><%= hero.name %></h1>
              </div>
              <div class="divider"></div>
              <div class="sixteen wide mobile only column">
                <div class="divider"></div>
              </div>
              <div class="sixteen wide column">
                  <div class="row">
                      <div class="inline-img">
                          <img src="https://liquipedia.net/commons/images/thumb/c/c6/Strength_Icon.png/30px-Strength_Icon.png" referrerpolicy="no-referrer" alt="">
                          <span><%= hero.attributes[0].start %> + <%= hero.attributes[0].grow %></span>
                      </div>
                   <div class="inline-img">
                       <img src="https://liquipedia.net/commons/images/thumb/f/f1/Agility_Icon.png/30px-Agility_Icon.png" referrerpolicy="no-referrer" alt="">
                       <span><%= hero.attributes[1].start %> + <%= hero.attributes[1].grow %></span>
                    </div>
                   <div class="inline-img">
                       <img src="https://liquipedia.net/commons/images/thumb/b/b0/Intelligence_Icon.png/30px-Intelligence_Icon.png" referrerpolicy="no-referrer"  alt="">
                       <span><%= hero.attributes[2].start %> + <%= hero.attributes[2].grow %></span>
                   </div>
                  </div>
              </div>
            </div>
          </div>
          </div>
        </div>
        <div class="row">
            <div class="row pt-1 pl-1">
            <!-- TIMELINE -->
            <div class="column six wide">
              <section>
              
                <ul class="timeline pl-1">
                    <% var buttons = ['Q', 'W', 'E', 'R']%>
                    <% var extraButtons = ['D', 'F']%>
                    <% if(hero.abilityImgLinks.length > 4) {%>
                        <% var Ultimate = buttons.pop();%>
                        <% if(hero.name != 'Invoker') {%>
                            <% hero.abilityImgLinks.length == 5? buttons.push(extraButtons[0]) : buttons = buttons.concat(extraButtons);%>
                            <% buttons.push(Ultimate);%>
                        <%}%>
                    <%}%>
                    <%for(let i = 0; i < hero.abilityImgLinks.length; i++) {%>
                  <li class="row skill">
                       <img referrerpolicy="no-referrer" class="column six" src="<%= hero.abilityImgLinks[i]%>" alt="">
                       <div class="column six">
                            <h4><%= buttons.length-1 < i? '' : buttons[i]%></h4>
                            <h6><%= hero.abilityDescriptions[i]%></h6>
                       </div>
                  </li>
                    <%}%>
                </ul>
              </section>
            </div>
          </div>
          <div class="column six wide pl-2">
            <!-- Languages -->
            <section class="talents">
              <div class="row rowSide">
                  <h3 id="talentHeader">Talents</h3>
                  <img id="talentImg" src="data:image/png;base64," alt="talents tree">
              </div>
              <div class="tags">
                  <ol type="1">
                      <%hero.talents.forEach(el => {%>
                          <li><%= el%></li>
                      <%})%>
                  </ol>
              </div>
            </section>
            <section class="startStats">
                <div class="row rowSide">
                    <h3>Start Stats</h3>
                </div>
                <div class="row">
                    <div class="column six">
                        <ul>
                            <li><b>Уровень </b></li>
                            <li><b>Здоровье </b></li>
                            <li><b>Восст.зд. </b></li>
                            <li><b>Мана </b></li>
                            <li><b>Восст.м. </b></li>
                            <li><b>Броня </b></li>
                            <li><b>Атак/сек. </b></li>
                            <li><b>Урон </b></li>
                        </ul>
                    </div>
                    <div class="column six">
                        <ul type="none">
                            <li>1</li>
                            <%hero.firstLvlStats.forEach(el=>{%>
                                <li><%= el%></li>
                            <%})%>

                        </ul>
                    </div>
                    <div class="column six">
                        <ul type="none">
                            <li>30</li>
                            <%hero.lastLvlStats.forEach(el=>{%>
                                <li><%= el%></li>
                            <%})%>
                        </ul>
                    </div>
                </div>
            </section>
            <section class="other">
                <div class="row rowSide">
                  <h3>Other stats</h3>
                </div>
                <div class="tags">
                  <ul style="list-style-type: none;">
                      <li>Сопротивление магии: <%=hero.defaultStats[0]%></li>
                      <li>Скорость передвижения: <%=hero.defaultStats[1]%></li>
                      <li>Скорость атаки: <%=hero.defaultStats[2]%></li>
                      <li>Скорость поворота: <%=hero.defaultStats[3]%></li>
                      <li>Дальность обзора: <%=hero.defaultStats[4]%></li>
                      <li>Дальность атаки: <%=hero.defaultStats[5]%></li>
                      <li>Скорость снаряда: <%=hero.defaultStats[6]%></li>
                      <li>Анимация атаки: <%=hero.defaultStats[7]%></li>
                      <li>Базовое время атаки: <%=hero.defaultStats[8]%></li>
                      <li>Блокировка урона: <%=hero.defaultStats[9]%></li>
                      <li>Размер столкновения: <%=hero.defaultStats[10]%></li>
                      <li>Ноги: <%=hero.defaultStats[11]%></li>
                      <li>Тип ошметков: <%=hero.defaultStats[12]%></li>
                  </ul>
              </div>
            </section>
        </div>
      </div>
    </div>
  </article>
    <%}%>
</main>
</body>
</html>
